  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'server'",
      "cargo": {
        "args": [
          "build",
          "--bin=server",
          "--package=server"
        ],
        "filter": {
          "name": "server",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}/apps/server",
      "env": {
        "SERVER_HTTP_SERVER_API_KEY" : "override-secret-here-env",
        "SERVER_HTTP_SERVER_URI" : "override-secret-here-env",
        "SERVER_VALUE_B_C_D" : "override-secret-here-env",
        "SERVER_HTTP_SERVER_C_D" : "override-secret-here-env",
      }
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'server'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=server",
          "--package=server"
        ],
        "filter": {
          "name": "server",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'books'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--lib",
          "--package=books"
        ],
        "filter": {
          "name": "books",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'common'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--lib",
          "--package=common"
        ],
        "filter": {
          "name": "common",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}